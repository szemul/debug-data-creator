{
    "name": "szemul/debug-data-creator",
    "description": "Debug file creating error handler",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "szemul/error-handler": "^2.0.0",
        "szemul/helper": "^1.1.0"
    },
    "require-dev": {
        "ext-posix": "*",
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\DebugDataCreator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\DebugDataCreator\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
